{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/**/*.{ts,tsx}",
    "wxt.config.ts"
  ],
  "project": ["**/*.{ts,tsx}"],
  "ignore": [
    "node_modules/**",
    ".output/**",
    ".wxt/**",
    "**/*.d.ts"
  ],
  "ignoreDependencies": [
    "tslib",
    "jscpd"
  ]
}
